                                <li><a href="@Url.Action("AccesssRoad", "Home")">Apply for Permit</a></li>
                                <li><a href="@Url.Action("PendingAccessRoad", "Home")">Pending Applications</a></li>
                                <li><a href="@Url.Action("VerifiedAccessRoad", "Home")">Verified Applications</a></li>
                                <li><a href="@Url.Action("RejectedAccessRoad", "Home")">Rejected Applications</a></li>
                                <li><a href="@Url.Action("ApprovedRoadAccess", "Home")">Approved Applications</a></li>
