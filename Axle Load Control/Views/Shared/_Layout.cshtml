                            <ul style="display: none;">
                                <li><a href="@Url.Action("Billboards", "Home")">Apply for Permit</a></li>
                                <li><a href="@Url.Action("OpenBillboards", "Home")">Open Applications</a></li>
                                <li><a href="@Url.Action("PendingBillboards", "Home")">Pending Applications</a></li>
