@model IEnumerable<Axle_Load_Control.Models.AxlePermitViewModel>
@{
    ViewBag.Title = "ApprovedAxles";
}

<div class="page-wrapper">
    <div class="content container-fluid">
        <div class="row">
            <div class="col-xs-4">
                <h4 class="page-title" style="width:400px">Approved Axle Applications</h4>
            </div>
            <div class="col-xs-8 text-right m-b-30">

                <a href="@Url.Action("AxlePermit", "Home")" class="btn btn-primary pull-right rounded"><i class="fa fa-plus"></i>Apply for Exemption Permit</a>
                <a href="@Url.Action("AxlePermit", "Home")" class="btn btn-primary pull-right rounded"><i class="fa fa-plus"></i>Approved Axle Permits</a>
                <a href="@Url.Action("AxlePermit", "Home")" class="btn btn-primary pull-right rounded"><i class="fa fa-plus"></i>Verified Axle Permits</a>
            </div>
        </div>
        <div class="row filter-row">
            <div class="col-sm-3 col-xs-6">
                <div class="form-group form-focus">
                    <label class="control-label">Permit ID</label>
                    <input type="text" class="form-control floating" />
                </div>
            </div>
            <div class="col-sm-3 col-xs-6">
                <div class="form-group form-focus">
                    <label class="control-label">Vehicle Registration</label>
                    <input type="text" class="form-control floating" />
                </div>
            </div>
            <div class="col-sm-3 col-xs-6">
                <div class="form-group form-focus">
                    <label class="control-label">Vehicle Make</label>
                    <input type="text" class="form-control floating" />
                </div>
            </div>
            <div class="col-sm-3 col-xs-6">
                <a href="#" class="btn btn-success btn-block"> Search </a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive">
                    <table class="table table-striped custom-table datatable">
                        <thead>
                            <tr>
                                <th>Ref No.</th>
                                <th>Vehicle Registration No.</th>
                                <th>Tare Weight</th>
                                <th>Vehicle Make/Type.</th>
                                <th>Region</th>
                                <th>Route</th>
                                <th>Status</th>
                                <th>Amount</th>
                                <th>Balance</th>
                                <th>View</th>
                            </tr>
                        </thead>
                        <tbody style="text-decoration-color:black">
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.refno)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.vehicleregistration)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tareweight)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.vehiclemake)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.region)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.route)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.status)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.totalamount)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.totalbalance)
                                    </td>
                                    <td><label class='btn btn-success'>View</label></td>
                                    @*<td><label class='btn btn-danger'>Reject</label> </td>*@
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
